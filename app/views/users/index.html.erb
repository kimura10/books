<div class="container">
 <div class="row">
   <%= render 'users/user', user: @user,book: @booknew %>
    <div class="col-md-8 offset-md-1">
     <h2>Users</h2>
     <table class="table">
      <thead>
       <th>image</th>
       <th>name</th>
      </thead>

         <% @users.each do |user| %>
          <tr>
          	<td><%= attachment_image_tag user, :profile_image, :fill, 50, 50,fallback: "no_image.jpg"%></td>
          	<td><%= user.name %></td>
          	<td><%= link_to "Show", "/users/#{user.id}" %></td>
          </tr>
      <% end %>
     </table>
    </div>
